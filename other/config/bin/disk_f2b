#!/bin/sh

dir=`pwd -P`

mv -v $dir/travmaturg/семейное $dir/backup/personal/latest || exit 1

mv -v $dir/travmaturg/archive $dir/backup/downloadable/latest
mv -v $dir/travmaturg/public $dir/backup/downloadable/latest

mkdir -p $dir/backup/other

mv -v $dir/travmaturg/porn $dir/backup/other
mv -v $dir/travmaturg/unsorted $dir/backup/other

